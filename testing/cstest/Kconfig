#
# For a description of the syntax of this configuration file,
# see the file kconfig-language.txt in the NuttX tools repository.
#

config TESTING_CSTEST
	tristate "cstest example"
	default n
	---help---
		Enable the cstest example.

if TESTING_CSTEST

config TESTING_CSTEST_PROGNAME
	string "Program name"
	default "cstest"
	---help---
		This is the name of the program that will be used when the NSH ELF
		program is installed.

config TESTING_CSTEST_PRIORITY
	int "cstest task priority"
	default 100

config TESTING_CSTEST_STACKSIZE
	int "cstest stack size"
	default DEFAULT_TASK_STACKSIZE

endif
